<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myphysicsapps">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logofisika"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logofisika"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPhysicsApps"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".catatan.NoteUpdate"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".catatan.Note"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".quiz.hasilQuizActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:launchMode="singleTop">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".quiz.QuizActivity"
            android:exported="false"
            android:launchMode="singleTop" />
        <activity
            android:name=".video.VideoActivity"
            android:exported="false"
            android:launchMode="singleTop" />
        <activity
            android:name=".materi.DetailActivityMateri"
            android:exported="false"
            android:launchMode="singleTop" />
        <activity
            android:name=".materi.MateriActivity"
            android:exported="false"
            android:launchMode="singleTop" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>